{
    "name": "fast/dev-tool",
    "description": "Dev tools package for Fast CMS application",
    "type": "package",
    "autoload": {
        "psr-4": {
            "Fast\\DevTool\\": "src"
        }
    },
    "require": {
        "jakub-onderka/php-parallel-lint": "dev-master",
        "squizlabs/php_codesniffer": "^3.3",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "phpmd/phpmd": "^2.8",
        "doctrine/dbal": "^2.9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fast\\DevTool\\Providers\\DevToolServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
